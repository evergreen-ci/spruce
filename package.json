{
  "name": "evergreen",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "7.1.0",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^4.1.2",
    "@svgr/webpack": "2.4.1",
    "@types/material-ui": "^0.21.6",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-router-dom": "^4.3.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.0",
    "babel-preset-react-app": "^5.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "evergreen.js": "^1.0.16",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin": "0.4.9",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "material-ui-dropzone": "^2.3.5",
    "mini-css-extract-plugin": "0.4.3",
    "moment": "^2.24.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.1.1",
    "react-dev-utils": "^6.0.1",
    "react-dom": "^16.8.6",
    "react-infinite-scroller": "^1.2.4",
    "react-router-dom": "^5.0.1",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "serve": "^10.1.2",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "thread-loader": "1.2.0",
    "ts-jest": "22.0.1",
    "ts-loader": "4.x.x",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "^3.7.2",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.1"
  },
  "scripts": {
    "start": "lessc src/styles.less src/styles.css && node scripts/start.js",
    "build": "lessc src/styles.less src/styles.css && node scripts/build.js",
    "test": "lessc src/styles.less src/styles.css && jest",
    "lint": "tslint -c tslint.json 'src/**/*.ts?(x)'",
    "lint-fix": "tslint --fix -c tslint.json 'src/**/*.ts?(x)'",
    "serve": "serve -s build",
    "dist": "mkdir -p bin && tsc scripts/dist.ts && node scripts/dist.js && rm scripts/dist.js",
    "update-snapshot": "jest --updateSnapshot"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.1",
    "@types/jest": "^24.0.15",
    "@types/node": "^11.13.17",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/tar": "^4.0.2",
    "@types/webpack": "^4.4.34",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest-junit": "^6.4.0",
    "less": "^3.9.0",
    "react-redux": "^6.0.1",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "tar": "^4.4.10",
    "typescript": "^3.5.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/src/test-shim.js",
      "<rootDir>/src/test-setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      "^.+\\.(ts|tsx)$": "<rootDir>/src/test-preprocessor.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
