{
  "name": "spruce",
  "version": "2.3.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js' --html | sed 1d > build/source_map.html",
    "build": "REACT_APP_GIT_SHA=`git rev-parse HEAD` node scripts/build.js --max-old-space-size=8192 build",
    "build-storybook": "build-storybook -s public",
    "build:prod": "env-cmd -e prod -r env/.cmdrc.json yarn build",
    "build:staging": "env-cmd -e staging -r env/.cmdrc.json yarn build",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "codegen": "graphql-codegen --config codegen.yml",
    "coverage": "yarn test -- --coverage --watchAll=false",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:test": "cypress run --spec",
    "deploy": "aws s3 sync build/ s3://${BUCKET}/ --acl public-read --follow-symlinks --delete --exclude env/.cmdrc.json",
    "deploy-prod:do-not-use-directly": "yarn build:prod && BUCKET=evergreen.spruce yarn deploy; yarn upload-source-maps;",
    "deploy:prod": "node cli-scripts/deploy-prod.js",
    "deploy:staging": "yarn build:staging && BUCKET=evergreen-staging.spruce yarn deploy",
    "dev": "env-cmd -e devServer -r env/.cmdrc.json yarn start",
    "eslint": "eslint -c .eslintrc.js 'src/**/*.ts?(x)' 'cypress/**/*.ts'",
    "eslint-fix": "STRICT=1 eslint -c .eslintrc.js --fix 'src/**/*.ts?(x)' 'cypress/**/*.ts'",
    "eslint-staged": "STRICT=1 eslint -c .eslintrc.js",
    "eslint:strict": "STRICT=1 eslint -c .eslintrc.js 'src/**/*.ts?(x)' 'cypress/**/*.ts'",
    "notify-email": "env-cmd -e prod -r env/.cmdrc.json node cli-scripts/notify-email.js",
    "prettier": "prettier --write",
    "prod": "env-cmd -e devProd -r env/.cmdrc.json yarn start",
    "staging": "env-cmd -e devStaging -r env/.cmdrc.json yarn start",
    "start": "rm -rf ./node_modules/.cache/babel-loader && node scripts/start.js",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "node scripts/test.js  --watchAll=false",
    "test:watch": "node scripts/test.js  --verbose",
    "upload-source-maps": "env-cmd -e prod -r env/.cmdrc.json node ./upload-bugsnag-sourcemaps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
