{
  "name": "spruce",
  "version": "2.36.4",
  "private": true,
  "scripts": {
    "build-storybook": "build-storybook -s public",
    "build:beta": "env-cmd -e beta -r env/.cmdrc.json yarn build",
    "build:local": "env-cmd -e local -r env/.cmdrc.json  yarn build",
    "build:prod": "env-cmd -e prod -r env/.cmdrc.json yarn build",
    "build:staging": "env-cmd -e staging -r env/.cmdrc.json yarn build",
    "build": "GIT_SHA=`git rev-parse HEAD` vite build",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "codegen": "graphql-codegen --config codegen.yml",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:test": "cypress run --spec",
    "deploy-prod:do-not-use-directly": "yarn build:prod && BUCKET=evergreen.spruce yarn deploy; yarn upload-source-maps;",
    "deploy:beta": ". ./appVersion.sh && yarn build:beta && BUCKET=evergreen-beta.spruce yarn deploy",
    "deploy:prod": "node cli-scripts/deploy-prod.js",
    "deploy:staging": ". ./appVersion.sh && yarn build:staging && BUCKET=evergreen-staging.spruce yarn deploy",
    "deploy": "aws s3 sync build/ s3://${BUCKET}/ --acl public-read --follow-symlinks --delete --exclude env/.cmdrc.json",
    "dev": "env-cmd -e devLocal -r env/.cmdrc.json yarn start",
    "eslint-fix": "STRICT=1 eslint -c src/.eslintrc.js --fix 'src/**/*.ts?(x)' && eslint -c cypress/.eslintrc.js 'cypress/**/*.ts'",
    "eslint-staged:cypress": "STRICT=1 eslint -c cypress/.eslintrc.js",
    "eslint-staged:src": "STRICT=1 eslint -c src/.eslintrc.js",
    "eslint-staged": "STRICT=1 eslint -c .eslintrc.js",
    "eslint:strict": "STRICT=1 eslint -c src/.eslintrc.js 'src/**/*.ts?(x)' && eslint -c cypress/.eslintrc.js 'cypress/**/*.ts'",
    "eslint": "eslint -c src/.eslintrc.js 'src/**/*.ts?(x)' && eslint -c cypress/.eslintrc.js 'cypress/**/*.ts'",
    "notify-email": "env-cmd -e prod -r env/.cmdrc.json bash email.sh",
    "prettier": "prettier --write",
    "prod": "env-cmd -e devProd -r env/.cmdrc.json yarn start",
    "serve": "node scripts/prodServer.js",
    "snapshot": "node scripts/test.js storybook.test.ts --watchAll=false",
    "staging": "env-cmd -e devStaging -r env/.cmdrc.json yarn start",
    "start": "vite",
    "storybook": "start-storybook -p 9009 -s public",
    "test:watch": "node scripts/test.js  --verbose",
    "test": "node scripts/test.js  --watchAll=false",
    "upload-source-maps": "env-cmd -e prod -r env/.cmdrc.json node ./upload-bugsnag-sourcemaps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
