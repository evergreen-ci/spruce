{
  "name": "new-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-testing": "^3.1.3",
    "@bugsnag/browser": "^6.5.0",
    "@bugsnag/plugin-react": "^6.5.0",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@leafygreen-ui/badge": "^3.0.0",
    "@leafygreen-ui/card": "^2.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.24",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "antd": "^3.26.7",
    "antd-table-infinity": "^1.1.6",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-retry": "^2.2.15",
    "apollo-link-schema": "^1.2.4",
    "axios": "^0.19.2",
    "env-cmd": "^10.0.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^4.0.6",
    "lodash.get": "^4.4.2",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.4",
    "waait": "^1.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:staging": "env-cmd -e staging -r config/.cmdrc.json npm run build",
    "build:prod": "env-cmd -e prod -r config/.cmdrc.json npm run build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "start:dev-server": "env-cmd -e devServer -r config/.cmdrc.json npm run start",
    "start:mock-instrospect-schema": "env-cmd -e mockIntrospectSchema -r config/.cmdrc.json npm run start",
    "start:mock-custom-schema": "env-cmd -e mockCustomSchema -r config/.cmdrc.json npm run start",
    "deploy": "aws s3 sync build/ s3://${BUCKET}/ --acl public-read --follow-symlinks --delete --exclude config.json",
    "lint": "tslint -c tslint.js 'src/**/*.ts?(x)'",
    "lint-fix": "tslint --fix -c tslint.js 'src/**/*.ts?(x)'",
    "eslint": "eslint -c .eslintrc.js 'src/**/*.ts?(x)'",
    "coverage": "npm run test -- --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/{serviceWorker.ts,index.tsx,react-app-env.d.ts}"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "cypress": "^3.8.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-graphql": "^3.1.1",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-graphql": "0.0.6",
    "tslint-react": "^4.1.0"
  },
  "proxy": "http://localhost:9090"
}
